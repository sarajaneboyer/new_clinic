<h1>Add a New Patient</h1><hr />

<%= form_with(model: @patient, local: true) do |f| %>
<div class="field">
  <%= f.label :first_name %>
  <%= f.text_field :first_name, required: true, autofocus: true %><br />

  <%= f.label :last_name %>
  <%= f.text_field :last_name, required: true %><br />
</div>
  <%= f.submit %>
  <% end %>